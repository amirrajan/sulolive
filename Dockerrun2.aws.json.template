{"AWSEBDockerrunVersion": 2,
 "volumes": [
   {
     "name": "sulo-live-webserver",
     "host": {
       "sourcePath": "/var/app/current"
     }
   }
 ],
 "authentication": {
    "bucket": "sulo-elb",
    "key": "docker/.dockercfg"
 },
 "containerDefinitions": [
   {
     "name": "sulo-live-webserver",
     "image": "<DOCKER_IMAGE>",
     "memory": "1500",
     "environment": [
       {
         "name": "ASSET_VERSION",
         "value": "<ASSET_VERSION>"
       }
     ],
     "portMappings": [
       {
         "hostPort": 80,
         "containerPort": 8080
       }
     ],
     "essential": true
   }
 ]
}
